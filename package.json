{
  "name": "indovel-restful-api",
  "version": "1.0.0",
  "description": "Toko online sederhana",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon main.js",
    "start": "npm main",
    "build": "babel src -d lib",
    "test": "jest"
  },
  "jest": {
    "maxConcurrency": 4,
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    },
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    },
    "collectCoverage": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mancode77/indovel-restful-api"
  },
  "keywords": [
    "api",
    "nodejs",
    "mysql"
  ],
  "author": "Aji Maulana",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.2",
    "formidable": "^2.0.1",
    "jest": "^29.0.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mv": "^2.1.1",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10"
  }
}
